        timer = duration;
        break;
    case CLOSING:
        // fade to clear, so end result is alpha = 0
        start = 0;
        timer -= closeSpeed;
        if (timer &lt; 0)
        {
            state = CLOSED;
            // when finished, destroy
            instance_destroy();
        }
        break;
    default:
        show_error("Invalid screen dimmer state: " + string(state), true);
        state = CLOSED;
        break;
}
